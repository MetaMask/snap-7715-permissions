{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "license": "(MIT-0 OR Apache-2.0)",
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "clean": "rimraf public",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "yarn eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "start": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@metamask/7715-permission-types": "0.2.0",
    "@metamask/delegation-toolkit": "0.13.0",
    "@metamask/providers": "22.1.0",
    "babel-plugin-styled-components": "2.1.4",
    "dotenv": "17.2.0",
    "react": "18.3.1",
    "react-dom": "18.2.0",
    "react-is": "18.3.1",
    "styled-components": "6.1.19",
    "viem": "2.31.7"
  },
  "devDependencies": {
    "@metamask/eslint-config": "12.2.0",
    "@metamask/eslint-config-browser": "12.1.0",
    "@metamask/eslint-config-jest": "12.1.0",
    "@metamask/eslint-config-nodejs": "12.1.0",
    "@metamask/eslint-config-typescript": "12.1.0",
    "@svgr/webpack": "8.1.0",
    "@types/jest": "27.5.2",
    "@types/react": "18.2.4",
    "@types/react-dom": "18.2.4",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsdoc": "48.11.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-promise": "6.6.0",
    "gatsby": "5.15.0",
    "gatsby-plugin-manifest": "5.15.0",
    "gatsby-plugin-styled-components": "6.15.0",
    "gatsby-plugin-svgr": "3.0.0-beta.0",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.18",
    "rimraf": "6.0.1",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^20 || >=22"
  }
}
